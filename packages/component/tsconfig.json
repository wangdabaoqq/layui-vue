{
  "compilerOptions": {
    "baseUrl": ".",

    "module": "esnext",
    "target": "es2015",
    "moduleResolution": "Node",

    "strictNullChecks": true,
    "strict": true,
    "allowJs": true,

    "resolveJsonModule": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "declarationDir": "types",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["ESNext","DOM"]
  }
}
